                          <h3>7.Application Work/BAnk Details</h3><br></h3>

                          <button type="submit" class="btn btn-success btn-lg" name="button" onclick="return validateForm()">Submit</button>
                          <a href="/admin/dashboard/list"><button type="button"class="btn btn-danger btn-lg" name="button" >List Users</button></a>


                  </form>


                          <script type="text/javascript">
                          function validateForm()
                          {
                            var a=document.forms["Form"]["username"].value;
                            var b=document.forms["Form"]["dowhat"].value;

                            if (a==null || a=="" || b==null || b=="")
                            {
                              alert("Please Fill All Required Field");
                              return false;
                            }
                          }
                          </script>
                        </div>
                    </div>
                    <a href="/home" class="btn btn-success btn-lg btn-block" role="button" onclick="return ValidateForm(this)"></span>Home</a>
                </div>
            </div>
